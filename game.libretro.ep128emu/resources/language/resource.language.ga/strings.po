msgctxt "Addon Summary"
msgid "Enterprise - 64/128 (ep128emu)"
msgstr "Enterprise - 64/128 (ep128emu)"

msgctxt "Addon Description"
msgid "ep128emu is a portable emulator of the Enterprise 64/128, ZX Spectrum 48/128, and Amstrad CPC 464/664/6128 computers."
msgstr "Is aithriseoir iniompartha é ep128emu do na ríomhairí Enterprise 64/128, ZX Spectrum 48/128, agus Amstrad CPC 464/664/6128."

# Empty string with ID 30000
msgctxt "#30001"
msgid "Main thread wait (ms)"
msgstr "Fanacht an phríomhshnáithe (ms)"

msgctxt "#30002"
msgid "High sound quality"
msgstr "Ardchaighdeán fuaime"

msgctxt "#30003"
msgid "Use accelerated SW framebuffer"
msgstr "Úsáid maolán fráma SW luathaithe"

msgctxt "#30004"
msgid "Enable resolution changes (requires restart)"
msgstr "Cumasaigh athruithe réitigh (tá atosú ag teastáil)"

msgctxt "#30005"
msgid "Border lines to keep when zooming in"
msgstr "Línte teorann le coinneáil agus tú ag zúmáil isteach"

msgctxt "#30006"
msgid "System ROM version (Enterprise only)"
msgstr "Leagan ROM an chórais (Fiontar amháin)"

msgctxt "#30007"
msgid "Player 1 Zoom button"
msgstr "Cnaipe súmála Imreoir 1"

msgctxt "#30008"
msgid "Player 1 Info button"
msgstr "Cnaipe Eolais Imreoir 1"

msgctxt "#30009"
msgid "Player 1 Autofire for button"
msgstr "Imreoir 1 Uath-lámhaigh don chnaipe"

msgctxt "#30010"
msgid "Player 1 Autofire repeat delay"
msgstr "Moill athrá uathoibríoch tine Imreoir 1"
